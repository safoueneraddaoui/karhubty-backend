meta {
  name: Change Password
  type: http
  seq: 7
}

put {
  url: http://localhost:8080/api/users/{{userId}}/password
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "oldPassword": "Test123!",
    "newPassword": "NewPassword123!"
  }
}

vars:pre-request {
  userId: {{userId}}
  token: {{token}}
}
